{% extends '::base.html.twig' %}
{% block content %}
    {% embed  'partials/panel.html.twig' %}
        {% block panel_content %}
            <div class="panel-heading"><h1>Aikštelių patvirtinimas</h1></div>
            <div class="panel-body">
                <div class="col-sm-12 col-lg-6">
                    {% if map is defined %}
                        <div id="map">
                            {{ google_map(map) }}
                        </div>
                    {% endif %}
                </div>
            </div>
        {% endblock %}
    {% endembed %}
{% endblock %}